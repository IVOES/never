
func main() -> int
{
    func loop(i -> int) -> int
    {
        let f1 = lambda func(a -> int) -> int { a * a }
        let f2 = lambda func(b -> int) -> int { b + b }
        let f3 = lambda func(c -> int) -> int { i * i };    
    
        i < 1000 ? 
            { 
                f2 = f1;
                f3 = f1;
                loop(i + 1) 
            } : 
            10
    };
    
    loop(0)
}

