
func printtab( tab[n] : float ) -> int
{
    func __print( i : int, tab[n] : float, val : float ) -> int
    {
        i < n ? __print( i + 1, tab, printf(tab[i]) ) : 0
    };
    __print( 0, tab, 0.0 )
}

func main() -> int
{
    printtab( [ 3.5, 5.5, 7.5 ] : float - [ 3.0, 4.0, 7.0 ] : float )
}

