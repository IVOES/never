/**
 * functional programming language
 * syntactically scoped
 * first class functions
 * call by value
 */
func min(int a, int b) -> int
{
    return (a < b) ? a : b;
}

func minCoeff(int coeff) -> (int, int) -> int
{
    func min(int a, int b) -> int
    {
        return (a < b) ? coeff * a : coeff * b;
    }

    return min;
}

func print(int a) -> int
{
    return 0;
}

func my_func(int a, int b) -> int
{
    return minCoeff(a + b);
}

func main()
{
    return print(my_func(10, 15));
}

