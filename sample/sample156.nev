
func printtab( tab[n] -> float ) -> int
{
    func __print( i -> int, tab[n] -> float, val -> float ) -> int
    {
        return i < n ? __print( i + 1, tab, printf(tab[i]) ) : 0;
    }
    return __print( 0, tab, 0.0 );
}

func main() -> int
{
    return printtab( - { 10.0, 20.0, -30.0, -40.0 } -> float );
}

