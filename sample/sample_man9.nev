
func dir_deg(d -> int, coeff -> float) -> (float) -> float
{
    func fah2cel(f -> float) -> float
    {
        coeff * ((f - 32.0) / 1.8)
    }
    func cel2fah(c -> float) -> float
    {
        coeff * (c * 1.8 + 32.0)
    };

    d == 0 ? cel2fah : fah2cel
}

func main() -> float
{
    dir_deg(0, 100.0)(100.0)
}

