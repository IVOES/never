
func tforeach( t[elems] -> int, each(e -> int) -> int) -> int
{
	func __tforeach( val -> int, i -> int, t[elems] -> int ) -> int
	{
	    i < elems ? __tforeach( each(t[i]), i + 1, t ) : 0
	};
	
	__tforeach(t[0], 0, t)
}

func main() -> int
{
	tforeach( [ 10, 20, 50, 30, 80, 99 ] -> int, print )
}


