
func get(c -> int) -> (float, float) -> float
{
    c ? func min(a -> float, b -> float) -> float
           {
              a < b ? a : b
           }
      : func max(a -> float, b -> float) -> float
           {
              a > b ? a : b
           }
}

func main() -> int 
{
    assert(get(0)(10.0, 20.0) == 20.0)
}



