
func get(int c) -> (float, float) -> float
{
    return c ? func min(float a, float b) -> float
               {
                   return a < b ? a : b;
               }
             : func max(float a, float b) -> float
               {
                   return a > b ? a : b;
               };
               
}

func main() -> int 
{
    return assert(get(0)(10.0, 20.0) == 20.0);
}



