
func get(float c) -> (float, float) -> float
{
    return c ? func min(float a, float b) -> float
               {
                   return a < b ? a : b;
               }
             : func max(float a, float b) -> float
               {
                   return a > b ? a : b;
               };
               
}

func main() -> float
{
    return assert(get(0)(10, 20) == 20);
}



