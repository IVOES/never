func no(float n) -> float
{
    return n;
}

func mutrec(float n, no(float) -> float) -> float
{
    func A(no(float) -> float, float n) -> float
    {
        return (n == 1.0) ? B(1.0, no) : 87.0;
    }
    
    func B(float n, no(float) -> float) -> float
    {
        return (n == 1.0) ? A(no, 10.0) : 12.0;
    }
    
    func C(float n) -> float
    {
        return A(no, n);
    }
    
    return C(n);
}

func main() -> int 
{
    return assert(mutrec(1.0, no) == 87.0);
}

