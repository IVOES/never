
func min(float a, float b) -> float
{
    return (a < b) ? a : b;
}

func max(float a, float b) -> float
{
    return (a > b) ? a : b;
}

func select(int a) -> (float, float) -> float
{
    return a < 100 ? min : max;
}

func main() -> int 
{
    return assert(select(110)(40.0, 20.0) == 40.0);
}


