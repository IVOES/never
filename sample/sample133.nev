
func exists( e -> int, t[elems] -> int ) -> int
{
	func __exists( i -> int, e -> int, t[elems] -> int ) -> int
	{
		return i < elems ? ( e == t[i] ? 1 : __exists( i + 1, e, t ) ) : 0;
	}
	return __exists( 0, e, t );
}

func main() -> int
{
	return exists( 100, [ 60, 20, 10, 30, 50, 40, 80, 90, 100 ] -> int );
}


