record R { value : int; }
 
enum Optional { Some { r : R; }, None }

func f() -> Optional
{
    Optional.Some(R(10))
}

func main() -> int
{
    let o = Optional.Some(R(10));

    match o
    {
        Optional.Some(r1, r2) -> print(20);
        Optional.None -> print(10);
    };
        
    0
}

