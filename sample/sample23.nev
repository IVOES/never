func calc(coeff -> int, n -> int) -> int
{
    func C() -> int 
    {
        return coeff;
    }

    func help(n -> int) -> int 
    {
        func fib(n -> int) -> int 
        {
            return ((n == 0) ? 1 : (n == 1) ? 1 : fib(n - 1) + fib(n - 2)) * C();
        }
        return fib(n);
    }
    
    return help(n);     
}

func main() -> int 
{
     return calc(2, 7);
}

