
record Point {
    x -> int;
    y -> int;
}

func get(r -> Point) -> int
{
    r.x = 100;
    r.y = 200;

    0
}

func printr(r -> Point) -> int
{
    prints("[" + r.x + ", " + r.y + "]\n");

    0
}

func test() -> int
{
    var r = new Point;

    get(r);
    
    assert(r.x == 100);
    assert(r.y == 200);
    
    0
}

func main() -> int
{
    test();

    0
}

