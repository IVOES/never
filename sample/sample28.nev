func no(float n) -> float
{
    return (-3) * n;
}

func mutrec(float n) -> float
{
    func A(float n) -> float
    {
        return (n == 1) ? B(1) : no(n);
    }
    
    func B(float n) -> float
    {
        return (n == 1) ? A(11) : 12;
    }
    
    func C(float n) -> float
    {
        return A(n);
    }
    
    return C(n);
}

func main() -> float
{
    return assert(mutrec(1) == -33);
}

