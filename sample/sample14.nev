/**
 * loads of global variables
 */
func fun0(a0 : float, b0 : float) -> float
{
    func fun1(a1 : float, b1 : float) -> float
    {
        func fun2(a2 : float, b2 : float) -> float
        {
            func fun3(a3 : float, b3 : float) -> float
            {
                a0 + a1 + a2 + a3 + b0 + b1 + b2 + b3
            }
            fun3(a2, b2)
        }
        fun2(a1, b1)
    }
    fun1(a0, b0)
}

func main() -> bool
{
    assert(fun0(2.0, 3.0) == 20.0)
}

