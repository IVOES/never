/**
 * loads of global variables
 */
func fun0(float a0, float b0) -> float
{
    func fun1(float a1, float b1) -> float
    {
        func fun2(float a2, float b2) -> float
        {
            func fun3(float a3, float b3) -> float
            {
                return a0 + a1 + a2 + a3 + b0 + b1 + b2 + b3; 
            }
            return fun3(a2, b2);
        }
        return fun2(a1, b1) ;
    }
    return fun1(a0, b0);
}


func main() -> float
{
    return assert(fun0(2, 3) == 20);
}

