/**
 * Passing function and calling returned function.
 */
func twice(float c1, float c2) -> (float, float) -> float
{
    func mult() -> float
    {
        return c2 == 0.0 ? 0.0 : c1 / c2;
    }

    func twice_func(float a, float b) -> float
    {
        func twice_func_nest(float a, float b) -> float
        {
            return mult();
        }
        return twice_func_nest(a, b);
    }
    
    return twice_func;
}

func main() -> float 
{
    return twice(12.0, 3.0)(10.0, 20.0);
}


