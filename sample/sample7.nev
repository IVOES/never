/**
 * Passing function and calling returned function.
 */
func min(int a, int b) -> int
{
    return a < b ? a : b;
}

func twice(single_func(int, int) -> int) -> (int, int) -> int
{
    func twice_func(int a, int b) -> int
    {
        return 2 * single_func(a, b);
    }
    
    return twice_func;
}

func main() -> int
{
    return twice(min)(10, 20);
}


