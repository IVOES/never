func mutrec(n -> float) -> float
{
    func A(n -> float) -> float
    {
        return (n == 1.0) ? B(1.0) : 99.0;
    }
    
    func B(n -> float) -> float
    {
        return (n == 1.0) ? A(10.0) : 12.0;
    }
    
    func C(n -> float) -> float
    {
        return A(n);
    }
    
    return C(n);
}

func main() -> int 
{
    return assert(mutrec(1.0) == 99.0);
}

