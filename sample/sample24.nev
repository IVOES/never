func mutrec(float n) -> float
{
    func A(float n) -> float
    {
        return (n == 1) ? B(1) : 99;
    }
    
    func B(float n) -> float
    {
        return (n == 1) ? A(10) : 12;
    }
    
    func C(float n) -> float
    {
        return A(n);
    }
    
    return C(n);
}

func main() -> float
{
    return assert(mutrec(1) == 99);
}

