
func min(float a, float b) -> float
{
    return (a < b) ? a : b;
}

func twice(single_func(float, float) -> float) -> (float, float) -> float
{
    func twice_func(float a, float b) -> float
    {
        func one_more() -> float
        {
            return 1000;
        }
    
        return one_more() * single_func(a, b);
    }

    return twice_func;
}

func main() -> float
{
    return twice(min)(12, 15);
}


