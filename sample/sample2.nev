
func min(a -> float, b -> float) -> float
{
    return (a < b) ? a : b;
}

func twice(single_func(float, float) -> float) -> (float, float) -> float
{
    func twice_func(a -> float, b -> float) -> float
    {
        func one_more() -> float
        {
            return 10.00;
        }
    
        return one_more() * single_func(a, b);
    }

    return twice_func;
}

func main() -> int
{
    return assert(twice(min)(12.0, 15.0) == 120.00);
}


