
func min(a : float, b : float) -> float
{
    (a < b) ? a : b
}

func twice(single_func(float, float) -> float) -> (float, float) -> float
{
    func twice_func(a : float, b : float) -> float
    {
        func one_more() -> float
        {
            10.00
        }
        one_more() * single_func(a, b)
    }
    twice_func
}

func main() -> int
{
    assert(twice(min)(12.0, 15.0) == 120.00)
}


