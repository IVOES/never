
func mul(a -> float, b -> float) -> float
{
    a * b
}

func funGen1(a -> float, b -> float) -> float
{
    func funInt(a -> float, b -> float) -> float
    {
        a * b
    }
    funInt(a, b + 2.0)
}

func funGen2(a -> float, b -> float) -> float
{
    mul(a, b + 2.0)
}

func main() -> int 
{
    assert(funGen2(10.0, 20.0) == 220.0)
}

