
func mul(float a, float b) -> float
{
    return a * b;
}

func funGen1(float a, float b) -> float
{
    func funInt(float a, float b) -> float
    {
        return a * b;
    }
    
    return funInt(a, b + 2);
}

func funGen2(float a, float b) -> float
{
