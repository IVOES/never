
func min(float a, float b) -> float
{
    return (a < b) ? a : b;
}

func funGen(float a, float b, f1(float a, float b) -> float) -> float
{
    return 2.0 * f1(a, b);
}

func funGen2(f1(float a, float b) -> float) -> (float a, float b) -> float
{
    func twice(float a, float b) -> float
    {
        return 2.0 * f1(a, b);
    }
    return twice;
}

func main() -> int 
{
    return assert(funGen2(min)(15.0, 20.0) == 30.0);
}


