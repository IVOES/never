

func dir_deg(int d) -> (float) -> float
{
    func fah2cel(float f) -> float
    {
        return (f - 32.0) / 1.8;
    }

    func cel2fah(float c) -> float
    {
        return c * 1.8 + 32.0;
    }

    return d == 0 ? fah2cel : cel2fah;
}

func main() -> int
{
    return dir_deg(0)(100.0);
}

